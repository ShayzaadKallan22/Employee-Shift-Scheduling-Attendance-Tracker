{
  "expo": {
    "name": "Azania",
    "slug": "azania-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Slide1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Slide1.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "splash": {
        "backgroundColor": "#000000",
        "resizeMode": "contain",
        "mdpi": "./assets/Slide1.png",
        "hdpi": "./assets/Slide1.png",
        "xhdpi": "./assets/Slide1.png",
        "xxhdpi": "./assets/Slide1.png",
        "xxxhdpi": "./assets/Slide1.png",
        "ldpi": "./assets/Slide1.png",
        "permissions": [
          "CAMERA",
          "READ_EXTERNAL_STORAGE",
          "WRITE_EXTERNAL_STORAGE"
        ]
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/Slide1.png",
        "backgroundColor": "#000000"
      },
      "package": "com.cletus_m.AzaniaApp",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bd31b921-c5c4-465c-ba00-d61777d52889"
      }
    },
    "owner": "cletus_tm",
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./node_modules/react-native-vector-icons/Fonts/Ionicons.ttf"
          ]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Azania to access your camera for QR code scanning."
        }
      ]
    ]
  }
}
